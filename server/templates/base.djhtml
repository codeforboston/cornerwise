<html>
    <head>
        <title>Cornerwise</title>
    </head>
    <body>
        {% if messages %}
            <ul class="messages" >
                {% for message in messages %}
                    <li class="message {{ message.tags }}">
                        {{ message }}
                    </li>
                {% endfor %}
            </ul>
        {% endif %}
        {% block body %}

        {% endblock %}
    </body>
</html>
